{
    "name": "hi/kernel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "spiral/core": "^3.12",
        "spiral/attributes": "^3.1",
        "spiral/exceptions": "^3.12",
        "spiral/debug": "^3.12",
        "spiral/files": "^3.12"
    },
    "autoload": {
        "psr-4": {
            "Hi\\": "src/"
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hi\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
